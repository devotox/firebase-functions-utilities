{
	"version": "1.0.0",
	"main": "dist/index.js",
	"name": "firebase-function-utilities",
	"description": "Firebase utilities",
	"author": "Devonte Emokpae",
	"license": "MIT",
	"directories": {
		"doc": "doc",
		"test": "tests"
	},
	"scripts": {
		"flow": "flow",
		"test": "npm run mocha",
		"lint": "eslint ./",
		"lint:one": "eslint",
		"prettier": "prettier --write",
		"jsinspect": "jsinspect ./src || true",
		"pretest": "npm run jsinspect && npm run lint",
		"mocha:one": "mocha --require babel-core/register",
		"mocha": "mocha --require babel-core/register --recursive tests",
		"precommit": "lint-staged",
		"postrelease": "npm publish",
		"release": "npm version patch",
		"release-patch": "npm version patch",
		"release-minor": "npm version minor",
		"release-major": "npm version major",
		"postversion": "git push && git push --tags",
		"build": "rm -rf ./dist/* && babel src -d dist",
		"dev": "rm -rf ./dist/* && babel -w src -d dist",
		"preversion": "sidekick run && npm run build && npm test"
	},
	"lint-staged": {
		"*.{js,json,css}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		]
	},
	"dependencies": {
		"compression": "1.7.1",
		"cors": "2.8.4",
		"express": "4.16.2",
		"express-redis-cache": "1.0.0",
		"fastboot": "1.1.3",
		"firebase-admin": "5.8.1",
		"firebase-functions": "0.8.1",
		"pg": "7.4.1",
		"pg-spice": "2.0.1",
		"request": "2.83.0"
	},
	"devDependencies": {
		"babel": "6.23.0",
		"babel-cli": "6.26.0",
		"babel-eslint": "8.2.1",
		"babel-plugin-transform-async-to-generator": "6.24.1",
		"babel-plugin-transform-flow-strip-types": "6.22.0",
		"babel-plugin-transform-object-rest-spread": "6.26.0",
		"babel-polyfill": "6.26.0",
		"babel-preset-env": "1.6.1",
		"babel-preset-flow": "6.23.0",
		"babel-preset-stage-1": "^6.24.1",
		"chai": "4.1.2",
		"eslint": "4.16.0",
		"eslint-config-prettier": "2.9.0",
		"eslint-plugin-ember-suave": "^1.0.0",
		"eslint-plugin-flowtype": "2.41.0",
		"eslint-plugin-prettier": "2.5.0",
		"flow-bin": "0.63.1",
		"jsinspect": "0.12.7",
		"mocha": "5.0.0",
		"prettier": "1.10.2",
		"sidekick": "1.12.2"
	}
}
