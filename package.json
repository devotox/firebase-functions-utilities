{
  "version": "2.0.1",
  "main": "dist/index.js",
  "name": "firebase-function-utilities",
  "description": "Firebase utilities",
  "author": "Devonte Emokpae",
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "flow": "flow",
    "test": "npm run mocha",
    "lint": "eslint ./",
    "lint:one": "eslint",
    "prettier": "prettier --write",
    "jsinspect": "jsinspect ./src || true",
    "pretest": "npm run jsinspect && npm run lint",
    "mocha:one": "mocha --require babel-core/register",
    "mocha": "mocha --require babel-core/register --recursive tests",
    "precommit": "lint-staged",
    "postrelease": "npm publish",
    "release": "npm version patch",
    "release-patch": "npm version patch",
    "release-minor": "npm version minor",
    "release-major": "npm version major",
    "postversion": "git push && git push --tags",
    "build": "rm -rf ./dist/* && babel src -d dist",
    "dev": "rm -rf ./dist/* && babel -w src -d dist",
    "preversion": "npm run build && npm test",
    "update:packages": "npx ncu -u --removeRange && yarn install --force --check-files --update-checksums --ignore-engines && yarn upgrade --force --ignore-engines"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "bluebird": "3.7.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-redis-cache": "1.1.3",
    "fastboot": "2.0.3",
    "firebase": "7.5.0",
    "firebase-admin": "8.8.0",
    "firebase-functions": "3.3.0",
    "pg": "7.11.0",
    "pg-spice": "2.0.1",
    "request": "2.88.0"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "10.0.3",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "4.2.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-ember-suave": "2.0.0-beta.1",
    "eslint-plugin-flowtype": "4.5.0",
    "eslint-plugin-prettier": "3.1.1",
    "flow-bin": "0.112.0",
    "jsinspect": "0.12.7",
    "mocha": "6.2.2",
    "prettier": "1.19.1"
  }
}
