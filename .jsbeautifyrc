{
  "html": {
	"brace_style": "collapse",
	"indent_char": " ",
	"indent_scripts": "normal",
	"indent_size": 4,
	"max_preserve_newlines": 1,
	"preserve_newlines": true,
	"indent_with_tabs": true,
	"unformatted": ["a", "sub", "sup", "b", "i", "u"],
	"wrap_line_length": 0
  },
  "css": {
	"indent_char": " ",
	"indent_size": 4,
	"indent_with_tabs": true
  },
  "js": {
	"indent_size": 4,
	"indent_char": " ",
	"indent_level": 0,
	"indent_with_tabs": true,
	"preserve_newlines": true,
	"max_preserve_newlines": 1,
	"jslint_happy": true
  },
  "sql": {
	"indent_size": 4,
	"indent_char": " ",
	"indent_level": 0,
	"indent_with_tabs": true
  }
}
